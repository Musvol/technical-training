<?xml version="1.0" ?>
<odoo>

    <record model="ir.ui.view" id="rental_tree_view">
        <field name="name">Book rentals</field>
        <field name="model">library.rental</field>
        <field name="priority" eval="16"/>
        <field name="arch" type="xml">
            
            <tree string="Rentals">
                <field name="customer_id"/>
                <field name="book_id"/>
            </tree>
        </field>
    </record>

    

</odoo>
